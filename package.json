{
  "name": "pipeline-cleaner",
  "version": "1.1.0",
  "description": "cleaner",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec tests --colors --recursive --exit",
    "start": "node app",
    "build": "./dockerfile/build.sh pipeline-cleaner"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "bahalool",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.7",
    "@hkube/etcd": "^4.0.16",
    "@hkube/logger": "^1.0.12",
    "moment": "^2.22.2",
    "requestretry": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.12.0",
    "express": "^4.16.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^5.1.1",
    "uuid": "^3.2.1"
  }
}