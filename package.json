{
  "name": "pipeline-cleaner",
  "version": "2.1.2",
  "description": "this service responsible for stopping expired pipelines",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test:cov": "./node_modules/nyc/bin/nyc.js --exclude=bootstrap.js --reporter=html --reporter=text --reporter=lcov npm run test",
    "start": "node app",
    "build": "./dockerfile/build.sh pipeline-cleaner"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "Hkube",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.10",
    "@hkube/consts": "^1.0.35",
    "@hkube/db": "^1.0.34",
    "@hkube/logger": "^1.0.26",
    "moment": "^2.29.1",
    "requestretry": "^4.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.2.3",
    "mockery": "^2.1.0",
    "nock": "^13.0.5",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "sinon": "^7.3.2"
  }
}
