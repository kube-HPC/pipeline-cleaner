{
  "name": "pipeline-cleaner",
  "version": "1.3.2",
  "description": "cleaner",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test:cov": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js --exclude=bootstrap.js --reporter=html --reporter=text --reporter=lcov mocha tests --colors --recursive --exit",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "node app",
    "build": "./dockerfile/build.sh pipeline-cleaner"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "bahalool",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.7",
    "@hkube/etcd": "^5.0.38",
    "@hkube/logger": "^1.0.29",
    "moment": "^2.24.0",
    "request": "^2.88.2",
    "requestretry": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "mockery": "^2.1.0",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "sinon": "^7.3.2"
  }
}
